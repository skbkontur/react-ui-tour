{
  "name": "react-ui-tour",
  "version": "1.0.0",
  "description": "react tours library",
  "repository": "https://github.com/skbkontur/react-ui-tour.git",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "node build.prod.js",
    "build-doc": "webpack --mode=production",
    "publish-doc": "rimraf dist && git clone -b gh-pages git@github.com:skbkontur/react-ui-tour.git dist && npm run build-doc && cd dist && git add --all && git commit --allow-empty -m \"published doc\" && git push && cd ..",
    "cmbuild": "npm i && npm run build",
    "storybook": "start-storybook -p 6006 --no-dll",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build-storybook": "build-storybook --no-dll"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "@skbkontur/react-ui": "^2.0.0",
    "react": ">=16.9 <17",
    "react-dom": ">=16.9 <17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@extern/tslint": "^1.2.0",
    "@skbkontur/react-ui": "^2.17.3",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^21.1.8",
    "@types/node": "^14.14.5",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^0.14.23",
    "@webpack-cli/serve": "^1.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "es3ify-webpack-plugin": "^0.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "jest-cli": "^26.6.1",
    "jest-css-modules-transform": "^1.0.3",
    "less": "^2.7.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.2.0",
    "react": "^16.14.0",
    "react-addons-css-transition-group": "^0.14.8",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-is": "^17.0.1",
    "react-scripts": "^4.0.0",
    "request": "^2.88.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.2",
    "ts-loader": "^8.0.7",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.44.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "keywords": [],
  "author": "Vladimir Tolstikov",
  "license": "ISC",
  "files": [
    "build/"
  ],
  "jest": {
    "setupFiles": [
      "./setupTests.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      ".+\\.(css|less)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
    },
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "raf": "^3.4.0"
  }
}
